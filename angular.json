{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "MangaOfflineViewerAngular": {
      "root": "",
      "projectType": "application",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "sourceMap": {
              "scripts": true,
              "styles": true,
              "vendor": true
            },
            "outputPath": "dist/MangaOfflineViewerAngular",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/app/shared/styles/buttons.css"
            ],
            "scripts": [],
            "serviceWorker": false
          },
          "configurations": {
            "production": {
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json",
              "optimization": true,
              "outputHashing": "all"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "MangaOfflineViewerAngular:build"
          }
        }
      }
    }
  },
  "defaultProject": "MangaOfflineViewerAngular",
  "cli": {
    "analytics": false
  }
}